{
  "name": "api",
  "version": "0.0.0",
  "description": "Node.js application (https://git.io/JYNud)",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rollup -c && (cd .build && yarn && rm .yarn/install-state.gz)",
    "lint": "eslint --cache --report-unused-disable-directives .",
    "test": "jest",
    "db:create": "node ./scripts/db-create && knex migrate:latest",
    "db:version": "knex migrate:currentVersion",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback 001_initial.js; knex migrate:latest",
    "db:backup": "node ./scripts/db-backup",
    "db:restore": "node ./scripts/db-restore",
    "db:repl": "node --experimental-repl-await ./scripts/repl",
    "update-types": "node ./scripts/update-types",
    "psql": "node ./scripts/psql",
    "repl": "node --experimental-repl-await ./scripts/repl",
    "deploy": "node ./scripts/deploy",
    "postinstall": "husky install"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cookie": "^0.4.1",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.4",
    "envalid": "^7.1.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.0",
    "google-auth-library": "^7.0.4",
    "got": "^11.8.2",
    "handlebars": "^4.7.7",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "nanoid": "^3.1.22",
    "pg": "^8.6.0",
    "simple-oauth2": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@jest/types": "^26.6.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-run": "^2.0.2",
    "@rollup/plugin-url": "^6.0.0",
    "@types/babel__core": "^7.1.14",
    "@types/cookie": "^0.4.0",
    "@types/email-templates": "^8.0.3",
    "@types/express": "^4.17.11",
    "@types/express-handlebars": "^3.1.0",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.41",
    "@types/simple-oauth2": "^4.1.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@yarnpkg/pnpify": "^3.0.0-rc.3",
    "babel-jest": "^26.6.3",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "knex-types": "^0.1.3",
    "prettier": "^2.2.1",
    "rollup": "^2.45.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
