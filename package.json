{
  "name": "api",
  "version": "0.0.0",
  "description": "Node.js application (https://git.io/JYNud)",
  "private": true,
  "main": "index.js",
  "scripts": {
    "setup": "husky install",
    "start": "rollup -c --watch",
    "build": "rollup -c",
    "lint": "eslint --cache --report-unused-disable-directives .",
    "test": "jest",
    "db:create": "node ./scripts/db-create && knex migrate:latest",
    "db:version": "knex migrate:currentVersion",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback 001_initial.js; knex migrate:latest && node ./scripts/update-types",
    "db:backup": "node ./scripts/db-backup",
    "db:restore": "node ./scripts/db-restore",
    "db:repl": "node --experimental-repl-await ./scripts/repl",
    "update-schema": "node ./scripts/update-schema",
    "update-types": "node ./scripts/update-types",
    "psql": "node ./scripts/psql",
    "repl": "node --experimental-repl-await ./scripts/repl",
    "deploy": "node ./scripts/deploy"
  },
  "dependencies": {
    "@google-cloud/logging": "^9.5.3",
    "chalk": "^4.1.1",
    "cookie": "^0.4.1",
    "dataloader": "^2.0.0",
    "email-templates": "^8.0.7",
    "envalid": "^7.1.1",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "express-handlebars": "^5.3.2",
    "google-auth-library": "^7.3.0",
    "got": "^11.8.2",
    "graphql": "^15.5.1",
    "graphql-relay": "^0.8.0",
    "handlebars": "^4.7.7",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.7",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "moment-timezone": "^0.5.33",
    "nanoid": "^3.1.23",
    "pg": "^8.6.0",
    "pretty-error": "^3.0.4",
    "simple-oauth2": "^4.2.0",
    "source-map-support": "^0.5.19",
    "validator": "^13.6.0",
    "validator-fluent": "^0.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@jest/types": "^27.0.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-run": "^2.0.2",
    "@rollup/plugin-url": "^6.0.0",
    "@types/babel__core": "^7.1.15",
    "@types/cookie": "^0.4.1",
    "@types/email-templates": "^8.0.4",
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^5.3.1",
    "@types/graphql-relay": "^0.7.0",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.3.2",
    "@types/simple-oauth2": "^4.1.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@yarnpkg/pnpify": "^3.0.0-rc.10",
    "babel-jest": "^27.0.6",
    "cross-spawn": "^7.0.3",
    "envars": "^0.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "knex-types": "^0.2.0",
    "prettier": "^2.3.2",
    "rollup": "^2.53.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "supertest": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "envars": {
    "cwd": "./env"
  }
}
