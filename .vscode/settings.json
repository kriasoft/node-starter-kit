{
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.nodePath": ".yarn/sdks",
  "files.associations": {
    ".env.**": "dotenv"
  },
  "jest.pathToJest": "yarn jest",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "sql-formatter.dialect": "pl/sql",
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "cSpell.ignoreWords": [
    "abcdefghijklmnopqrstuvwxyz",
    "browserslist",
    "citext",
    "cloudfunctions",
    "cloudsql",
    "corejs",
    "dataloader",
    "datname",
    "devtool",
    "downlevel",
    "envalid",
    "envars",
    "eslintcache",
    "esmodules",
    "esnext",
    "idps",
    "gamecenter",
    "GraphQLID",
    "gsutil",
    "jsonb",
    "knexfile",
    "kriasoft",
    "linkedin",
    "pathinfo",
    "pgappname",
    "pgdatabase",
    "pgdebug",
    "pghost",
    "pgpassword",
    "pgport",
    "pgservername",
    "pgsslcert",
    "pgsslkey",
    "pgsslmode",
    "pgsslrootcert",
    "pguser",
    "playgames",
    "pnpify",
    "postgres",
    "psql",
    "sendgrid",
    "sourcemap",
    "spdx",
    "tslib",
    "typechecking",
    "upsert",
    "webflow",
    "yarnpkg",
    "yarnrc"
  ]
}
